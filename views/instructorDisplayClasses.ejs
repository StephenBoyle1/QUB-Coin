<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Instructor Classes</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</head>

<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                    data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/instructorDashboard">Home</a>


        </div>

</div>
</nav>
</body>
</html>


<div class="page-header">
    <h1>View Classes</h1>
</div>

<h3>Classes<span class="label label-default"></span></h3>

<table class="table table-striped" dir="ltr" width="500" border="1"
       summary="purpose/structure for speech output">


    <colgroup id="colgroup" class="colgroup" align="center"
              valign="middle" title="title" width="30%"
              span="2"/>
    <thead>
    <tr>
        <th scope="col">Class Name</th>
        <th scope="col">Date/Time</th>
        <th scope="col">Location</th>
        <th scope="col">Instructor</th>
        <th scope="col">Class Type</th>


    </tr>
    </thead>

    <tfoot>
    <tr>
        <% classes.classesCreatedTable.forEach(function(item){ %>
        <td><%= item.className %></td>
        <td><%= item.timestamp %></td>
        <td><%= item.location %></td>
        <td><%= item.instructor %></td>
        <td><%= item.classType %></td>


    </tr>
    </tfoot>
    <tbody>

    <% }); %>
    </tbody>
</table>


</body>
</html>

